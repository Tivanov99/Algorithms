using System;
using System.Linq;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test(-1, 1, 0));
        }
        public static bool Test(int x, int y, int z)
        {
            return y > x && z > y ? true : false;
        }
    }
}
