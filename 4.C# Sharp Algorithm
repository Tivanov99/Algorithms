using System;

namespace Basic_Algorithm
{
    class Program
    {
        static void Main(string[] args)
        {
            InputReader();
            //4. Write a C# Sharp program to check a given integer
            //and return true if it is within 10 of 100 or 200
        }
        public static void InputReader()
        {
            int n = int.Parse(Console.ReadLine());
            OuputPrinter(n);
        }
        public static bool CalculateResult(int n)
        {
            return Math.Abs(n - 100) <= 10 || Math.Abs(n - 200) <= 10 ? true : false;
        }
        public static void OuputPrinter(int n)
        {
            Console.WriteLine(CalculateResult(n));
        }
    }
}
