using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Difference(89));
        }
        public static bool Difference(int x)
        {
            return Math.Abs(x - 100) <= 10 || Math.Abs(x - 200) <= 10 ? true : false;
        }
    }
}
