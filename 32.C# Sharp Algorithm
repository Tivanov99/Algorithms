using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Cointans(new int [] { 1, 2, 9, 3 }, 3));
        }
        public static bool Cointans(int [] array, int num)
        {
            for (int i = 0; i < array.Length; i++)
            {
                if (array[i] == num)
                {
                    return true;
                }
            }
            return false ;
        }
    }
}
