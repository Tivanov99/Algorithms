using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Swap("Red"));
        }
        public static string Swap(string input)
        {
            return input.Substring(input.Length - 1, 1) + input + input.Substring(input.Length - 1, 1);
        }
    }
}
