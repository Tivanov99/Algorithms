using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Cointans(new int [] { 1, 2, 9, 3 }, 3));
        }
        public static bool Cointans(int [] array, int num)
        {
            int length = array.Length-1 <=3 ? array.Length-1 : array.Length-1;

            for (int i = 0; i <= length; i++)
            {
                if (array[i] == num)
                {
                    return true;
                }
            }
            return false ;
        }
    }
}
