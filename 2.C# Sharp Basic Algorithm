using System;

namespace Basic_Algorithm
{
    class Program
    {
        static void Main(string[] args)
        {
            InputReader();
        }
        public static void InputReader()
        {
            int first = int.Parse(Console.ReadLine());
            CalculateResult(first);
        }
        public static bool IsTheSame(int first, int second)
        {
            if (first == second)
            {
                return true;
            }
            return false;
        }
        public static void CalculateResult(int n)
        {
            OuputPrinter(n > 51 ? (n-51) * 3 : 51-n);
        }
        public static void OuputPrinter(int sum)
        {
            Console.WriteLine(sum);
        }
    }
}
