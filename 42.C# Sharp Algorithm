using System;
using System.Linq;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test(15));
        }
        public static bool Test(int num1)
        {
            return num1 % 13 == 0 || num1 % 13 == 1;
        }
    }
}
