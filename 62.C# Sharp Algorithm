using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test("Hello"));
        }
        public static string Test(string s1)
        {
            string last = s1.Substring(s1.Length - 2, 2);
            return last + last + last;
        }
    }
}
