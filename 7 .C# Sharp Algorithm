using System;
using System.Text;

namespace Basic_Algorithm
{
    class Program
    {
        static void Main(string[] args)
        {
            InputReader();
            //Write a C# Sharp program to exchange the
            //first and last characters in a given string and return the new string.
        }
        public static void InputReader()
        {
            string n = Console.ReadLine();
            OuputPrinter(n);
        }
        public static void OuputPrinter(string n)
        {
            Console.WriteLine(Replacer(n));
        }
        public static string Replacer(string str)
        {
            return str.Length > 1 ?
                 str.Substring(str.Length - 1) + str.Substring(1, str.Length - 2) + str.Substring(0, 1) : str;
        }
    }
}
