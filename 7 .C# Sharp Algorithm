using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Swap("abcd"));
        }
        public static string Swap(string input)
        {
            return input.Length > 1 ? input.Substring(input.Length - 1, 1) + input.Substring(1, input.Length - 2)
                    + input.Substring(0, 1) : input;
        }
    }
}
