using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Concat("Python"));
        }
        public static string Concat(string x)
        {
            string result = string.Empty;

            for (int i = 0; i < x.Length; i++)
            {
                result += x.Substring(0, i + 1);
            }
            return result;
        }
    }
}
