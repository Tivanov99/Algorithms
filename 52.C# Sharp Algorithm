using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test(10, 10));
        }
        public static int Test(int x, int y)
        {
            return x > y ? x : (y > x ? y : (x % 7 == y % 7 ? (x < y ? x : y) : 0));
        }
    }
}
