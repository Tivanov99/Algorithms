using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Concat("1"));
        }
        public static string Concat(string input)
        {
            return input.Length > 2 ? input.Substring(0, 3) + input + input.Substring(0, 3) :
                 input.Substring(0, input.Length) + input + input.Substring(0, input.Length);
        }
    }
}
