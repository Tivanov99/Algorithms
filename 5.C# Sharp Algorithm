using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Difference(""));
        }
        public static string Difference(string x)
        {
            if(x.Length >= 2 && x.Substring(0, 2) == "if")
            {
                return x;
                
            }
            return "if " + x;
        }
    }
}
