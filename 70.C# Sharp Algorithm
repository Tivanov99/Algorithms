using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test("Hello"));
        }
        public static string Test(string l)
        {
            return l.Length >= 2 ? l.Remove(0, 1).Remove(l.Length - 2, 1) : l;
        }
    }
}
