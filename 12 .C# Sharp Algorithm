using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Concat("C++"));
        }
        public static bool Concat(string input)
        {
            return input.Length > 1 ? input.Substring(0, 2) == "C#" : false;
        }
    }
}
