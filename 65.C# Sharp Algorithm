using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Test("Python"));
        }
        public static string Test(string s1)
        {
            return s1.Length <= 2 ? string.Empty : s1.Remove(0, 1).Remove(s1.Length - 2, 1);
        }
    }
}
