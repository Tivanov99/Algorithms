using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Difference(20,25));
        }
        public static bool Difference(int x, int y)
        {
            return x == 30 || y == 30 ? true : x + y == 30;
        }
    }
}
