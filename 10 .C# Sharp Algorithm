using System;
using System.Text;

namespace Basic_Algorithm
{
    class Program
    {
        static void Main(string[] args)
        {
            InputReader();
            //10. Write a C# Sharp program to check if a given positive number is
            // a multiple of 3 or a multiple of 7
        }
        public static void InputReader()
        {
            int n = int.Parse(Console.ReadLine());
            OuputPrinter(n);
        }
        public static void OuputPrinter(int n)
        {
            Console.WriteLine(Editor(n));
        }
        public static bool Editor(int n)
        {
            return n % 3 == 0 || n % 7 == 0 ? true : false;
        }
    }
}
