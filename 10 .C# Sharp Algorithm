using System;
using System.Text;

namespace Basic_Algorithm
{
    class Program
    {
        static void Main(string[] args)
        {
            InputReader();
            //9. Write a C# Sharp program to create a new string with the last
            //char added at the front and back of a given string of length 1 or more.
        }
        public static void InputReader()
        {
            int n = int.Parse(Console.ReadLine());
            OuputPrinter(n);
        }
        public static void OuputPrinter(int n)
        {
            Console.WriteLine(Editor(n));
        }
        public static bool Editor(int n)
        {
            return n % 3 == 0 || n % 7 == 0 ? true : false;
        }
    }
}
