using System;
using System.Linq;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Sum(2,17));
        }
        public static int Sum(int num1, int num2)
        {
            return num1 + num2 >= 10 && num1 + num2 <= 20 ? 30 : num1 + num2;
        }
    }
}
