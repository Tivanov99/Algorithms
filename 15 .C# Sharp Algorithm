using System;

namespace Opp_Train
{
    public class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(Concat(11,20,12));
        }
        public static bool Concat(int x ,int y,int n)
        {
            return (x >= 20 && x <= 50) || (y >= 20 && y <= 50) || (n >= 20 && n <= 50);
        }
    }
}
